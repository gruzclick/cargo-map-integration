{
  "tests": [
    {
      "name": "Save single vehicle successfully",
      "method": "POST",
      "path": "/",
      "headers": {
        "X-User-Id": "test_user_123"
      },
      "body": {
        "vehicles": [
          {
            "driver_name": "Иванов Иван Иванович",
            "driver_phone": "+79991234567",
            "driver_license_number": "7711123456",
            "car_brand": "ГАЗель",
            "car_model": "Next",
            "car_number": "А123БВ199",
            "capacity_boxes": 100,
            "capacity_pallets": 4
          }
        ]
      },
      "expectedStatus": 200,
      "expectedBody": {
        "success": true,
        "saved_vehicles": "array"
      },
      "bodyMatcher": "partial"
    },
    {
      "name": "Missing X-User-Id header",
      "method": "POST",
      "path": "/",
      "body": {
        "vehicles": []
      },
      "expectedStatus": 401,
      "expectedBody": {
        "success": false,
        "error": "string"
      },
      "bodyMatcher": "partial"
    }
  ]
}
