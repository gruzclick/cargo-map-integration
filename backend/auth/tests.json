{
  "tests": [
    {
      "name": "Register client - email taken",
      "method": "POST",
      "path": "/",
      "body": {
        "action": "register",
        "email": "test@example.com",
        "password": "testpass123",
        "full_name": "Test User",
        "user_type": "client",
        "entity_type": "individual",
        "phone": "+79991234567"
      },
      "expectedStatus": 400,
      "expectedBody": {
        "error": "Email already registered"
      },
      "bodyMatcher": "partial"
    },
    {
      "name": "Login with invalid credentials",
      "method": "POST",
      "path": "/",
      "body": {
        "action": "login",
        "email": "wrong@example.com",
        "password": "wrongpass"
      },
      "expectedStatus": 401,
      "expectedBody": {
        "error": "Invalid credentials"
      },
      "bodyMatcher": "partial"
    }
  ]
}